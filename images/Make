#/bin/sh

WWWDIR=/Users/tms/Sites/acct
if [ ! -d $WWWDIR/images ]
then
	mkdir $WWWDIR/images
fi

for i in Spring Summer Fall Winter
do
	cp -pv $i*.jpg $WWWDIR/images
done
cp -pv ShowImages.html $WWWDIR

WWWDIR=/Users/tms/Sites/shs
if [ ! -d $WWWDIR/images ]
then
	mkdir $WWWDIR/images
fi

cp -pv shs*.png $WWWDIR/images
cp -pv ShowDarks.html $WWWDIR

